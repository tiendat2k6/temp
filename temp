int sdc(int a)
{
    if (a < 10) return 1;
    if (a == 1000) return a;

    if (a >= 10 && a < 100)
    {
        if (a % 10 == a / 10) return 11;
        else return 10;
    }

    int y = (a/10) % 10;
    int x = a/100, z = a % 10;

    if (x == y && y == z) return 111;
    if (x != y && y != z && z != x) return 102;

    if (x == y) return 110;
    if (x == z) return 101;

    return 100;
}

void solve() 
{
    int n; cin >> n;

    int ans = 0;
    for(int i = 1; i <= n; i++)
        ans += sdc(i);

    cout << ans;
}

int32_t main() 
{
    Fast;
    File("DANGCAU")
    // freopen("test.inp", "r", stdin);
    // int t; cin >> t; while(t--)
        solve();
    End;
}
