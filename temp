using namespace std;

const int N = 1e2 + 5;

/*-------------// Variables \\--------------*/
    int n; string s;
    map <string, bool> d;
/*------------------------------------------*/

void solve() 
{
    cin >> n >> s; s = ' ' + s;

    int ans = 0;
    for(int len = 1; len <= n; len++)
    {
        d.clear();
        bool ok = true;

        for(int i = 1; i <= n; i++)
        {
            string t = "";
            for(int j = i; j <= i+len-1; j++)
                t = t + s[j];

            if (d[t]) ok = false;
            d[t] = true;
        }

        if (ok) {ans = len; break;}
    }

    cout << ans;
}

int32_t main() 
{
    Fast;
    File("DIFFSSTR")
    // freopen("test.inp", "r", stdin);
    // int t; cin >> t; while(t--)
        solve();
    End;
}
